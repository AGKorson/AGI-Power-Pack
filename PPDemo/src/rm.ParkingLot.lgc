if (egoCarSeq) {
  [ ego arrives
  if (egoCarSeq == EC_ARRIVE) {
    [ disallow menu use
    reset(enableMenu);
    [ game functions are disabled
    set(disableGameFunctions);
    [ no input allowed
    prevent.input();
    
    [ move car to parking spot
    ++egoCarSeq;
    position(egoCar, 0, 131);
    draw(egoCar);
    move.obj(egoCar, 40, 143, 1, fCarDone);
    [ restore cursor for message
    cursoricon = cursormode;
    set.key(BYVAR, &cursoricon, mouse); [ set.cursor(FN_MODE, MSGNUM)
    print("At long last, you finally made it to Oakhurst, "
          "California to visit the original Sierra On-Line headquarters "
          "building.");
    print("It has been a long drive, but you are super excited as you "
          "pull into the parking lot.");
          
    [ show wait cursor
    cursoricon = C_WAIT;
    set.key(BYVAR, &cursoricon, mouse); [ set.cursor(FN_MODE, MSGNUM)
    [ play car-running sound, force 4-channel FM
    set(4channelsound);
    set.key(FM_MODE, 0, SET.SNDMODE); [ set.sndmode(MODE)
    [ use correct instrument
    work1 = 0;
    work2 = CAR_INSTRUMENT;
    set.key(&work1, &work2, SET.INST); [ set.inst(vCHANNEL, vINSTRUMENT)
    sound(s.Car, soundDone);
    set(playSound);
  }
  if (egoCarSeq == EC_PARKING && soundDone) {
    [ continue playing engine sound until final position reached
    [ (sound parameters are already set, so just play the sound)
    sound(s.Car, soundDone);
  }
  if (egoCarSeq == EC_PARKING && fCarDone) {
    [ stop car, play engine shutdown sound
    [ (sound parameters are already set, so just play the sound)
    reset(fCarDone);
    sound(s.CarStop, soundDone);
    stop.cycling(egoCar);
    egoCarSeq = EC_PARKED;
  }
  if (egoCarSeq == EC_PARKED && soundDone) {
    [ ego gets out
    reset(soundDone);
    egoCarSeq = EC_IDLE;
    position(ego, 50, 139);
    draw(ego);
    [ restore cursor for message
    cursoricon = cursormode;
    set.key(BYVAR, &cursoricon, mouse); [ set.cursor(FN_MODE, MSGNUM)
    [ check for weekend
    if ((weekday == 0 || weekday == 6)) {
      if (weekday == 0) {
        theDay = "Sunday";
      } else {
        theDay = "Saturday";
      }
      weekendMsg = "What luck! The clinic is open on %s12! ";
    } else {
      weekendMsg = "";
    }
    print("The building is now a medical clinic. %s13There aren't too many "
          "cars in the parking lot. So you decide to go inside and "
          "take a look around.");
    print("Who knows? Maybe you might find some original Sierra memorabilia "
          "or other signs of long past gaming history.");
    [ allow menu use
    set(enableMenu);
    [ game functions are no longer disabled
    reset(disableGameFunctions);
    [ allow input
    accept.input();
  }
  [ skip rest of room logic until opening sequence is finished
  if (egoCarSeq < EC_PARKED) {
    return();
  }
  
  [ ego drives away
  if (egoCarSeq == EC_LEAVE) {
    [ if walkman is in use, stop it
    if (currentTrack > 0 && currentTrack < 128) {
      currentTrack *= -1;
      stop.sound();
      reset(musicDone);
    }
    [ play car-start sound, force 4-channel FM
    set(4channelsound);
    set.key(FM_MODE, 0, SET.SNDMODE); [ set.sndmode(MODE)
    [ use correct instrument
    work1 = 0;
    work2 = CAR_INSTRUMENT;
    set.key(&work1, &work2, SET.INST); [ set.inst(vCHANNEL, vINSTRUMENT)
    sound(s.CarStart, soundDone);
    set(playSound);
    egoCarSeq = EC_STARTCAR;
  }
  if (egoCarSeq == EC_STARTCAR && soundDone) {
    [ ego drives away
    egoCarSeq = EC_LEAVING;
    start.cycling(egoCar);
    move.obj(egoCar, 143, 126, 2, fCarDone);
    [ (sound parameters are already set, so just play the sound)
    sound(s.Car, soundDone);
    set(playSound);
  }
  if (egoCarSeq == EC_LEAVING && soundDone) {
    [ keep playing car-running sound
    [ (sound parameters are already set, so just play the sound)
    sound(s.Car, soundDone);
    set(playSound);
  }
  if (egoCarSeq == EC_LEAVING && fCarDone) {
    reset(fCarDone);
    [ stop sound
    stop.sound();
    reset(playSound);
    [ ego is done; begin end-of-game sequence
    [ (using death handler)
    egoCarSeq = EC_IDLE;
    erase(egoCar);
    print("You drive away, your visit to Sierra On-Line is over.");
    [ set deathType
    deathType = DT_DRIVE_AWAY;
  }  
}

